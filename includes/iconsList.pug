-
    const icons = {
        free: [
            {
                name: 'robot',
                 url: './icons/robot.svg',
            },
            {
                name: 'car',
                url: './icons/car.svg',
            }
        ],
        premium: [
            {
                name: 'cat',
                url: './icons/cat.svg',
            },
            {
                name: 'dog',
                url: './icons/dog.svg',
            }
        ]
    };

mixin buttonLook(name = 'Картинку')
    button.btn.btn-order= 'Посмотреть ' + name
each pics, accesses in icons 
    h2= accesses 
    ul 
        each icon in pics 
            li
                a(href=icon.url)= icon.name
                +buttonLook(icon.name)